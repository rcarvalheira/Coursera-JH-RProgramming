complete <- function(directory, id = 1:332){
grouping <- matrix(, nrow = 0, ncol = 2)
colnames(grouping) <- c("id", "nbos")
for (i in id){
reading <- read.csv(paste(directory,
if (i < 10){
"/00"
}else if (i <100){
"/0"
}else{
"/"
},
i,
".csv",sep = ""),
colClasses = c("character",
"numeric",
"numeric",
"integer"),
dec=".")
reading <- reading[complete.cases(reading),]
nbos <- nrow(reading)
grouping <- rbind(grouping, c(i,nbos))
}
grouping
}
